<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nhj.springProjectmelon.repository.MusicRepository">

<select id="findAll"
		resultType="com.nhj.springProjectmelon.model.music.Music">
		select * from music order by id
	</select>

	<insert id="save">
		insert into
		music(title,singer,album,photo)
		values(#{title},#{singer},#{album},#{photo})
	</insert>

</mapper>